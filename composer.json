{
    "name": "vanterbit/yii2-ckeditor",
    "description": "Yii2 CKEditor - fork from MihailDev/yii2-ckeditor and improvements",
    "type": "yii2-extension",
    "require": {
        "yiisoft/yii2": "*"
    },
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Aleksandr Protchenko",
            "email": "protchenko.88@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {"vanterbit\\ckeditor\\": ""}
    },
    "extra": {
		"asset-installer-paths": {
			"npm-asset-library": "vendor/npm",
			"bower-asset-library": "vendor/bower"
		}
    },
    "minimum-stability": "stable"
}
